<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafxml2bindings.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>
    <TextField fx:id="inserimento" layoutX="115.0" layoutY="69.0" prefHeight="31.0" prefWidth="50.0" />
    <TextField fx:id="visualizza" layoutX="115.0" layoutY="105.0" prefHeight="31.0" prefWidth="50.0" />
</VBox>
